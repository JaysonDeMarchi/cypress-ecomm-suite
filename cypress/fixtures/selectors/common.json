{
  "action": {
    "checkout": {
      "enter_manual_address": ".enter-manual-address",
      "shipping_continue": "[data-role=opc-continue]",
      "payment_continue": "[id=authnetcim-continue]",
      "place_order": "[id=authnetcim-submit]"
    },
    "pdp": {
      "add_to_cart": "#product-addtocart-button"
    }
  },
  "field": {
    "checkout": {
      "shipping_address": {
        "firstname": "[name=firstname]",
        "lastname": "[name=lastname]",
        "street_0": "input[name^=street][name*=0]",
        "street_1": "input[name^=street][name*=1]",
        "city": "input[name=city]",
        "region": "select[name=region_id]",
        "postcode": "input[name=postcode]",
        "country": "[name=country_id]",
        "email": "[name=shipping-email]",
        "phone_number": "[name=telephone]"
      },
      "credit_card": {
        "number": "input[id=authnetcim-cc-number]",
        "exp_month": "select[id=authnetcim-cc-exp-month]",
        "exp_year": "select[id=authnetcim-cc-exp-year]",
        "cvv": "input[id=authnetcim-cc-cid]"
      }
    }
  },
  "info": {
    "pdp": {
      "add_to_cart_success": ".message-success"
    }
  }
}
